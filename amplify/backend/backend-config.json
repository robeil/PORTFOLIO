{
  "api": {
    "amplifyAPI": {
      "dependsOn": [
        {
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "function",
          "resourceName": "amplifyLambda"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "function": {
    "amplifyLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "hosting": {
    "S3AndCloudFront": {
      "providerPlugin": "awscloudformation",
      "service": "S3AndCloudFront"
    }
  }
}